package forloop;

public class Cube {

	public static void main(String[] args) {

		for (int sh = 1; sh <= 1000; sh++) {

			int count = 0;

			for (int num = sh; num > 0; num /= 10)
				count++;

			int temp = sh;
			int Finalans = 0;

			while (sh > 0) {
				int a = sh % 10;
				int ans = 1;

				for (int i = 1; i <= count; i++)
					ans *= a;

				Finalans += ans;
				sh /= 10;
			}

			if (temp == Finalans)
				System.out.println(temp + " is an ArmStrong Number");
			else
				System.out.println("Not An ArmStrong");
		}
	}
}
