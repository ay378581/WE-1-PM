package forloop;

public class Cube {

	public static void main(String[] args) {

		int num = 153;

		int temp = num;
		int Finalans = 0;
		
		while (num > 0) {
			int a = num % 10;
			int ans = 1;

			for (int i = 1; i <= 3; i++)
				ans *= a;

			Finalans += ans;
			num /= 10;
		}
		
		
		if(temp == Finalans)
			System.out.println(temp+" is an ArmStrong Number");
		else
			System.out.println("Not An ArmStrong");
	}
}
